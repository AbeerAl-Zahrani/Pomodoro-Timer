// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model CustomTimer {
  id String @id @default(uuid())
  timer Int
  longBreakTimer Int
  shortBreakTimer Int
  tasks Task[]
}
model Task {
  task String
}